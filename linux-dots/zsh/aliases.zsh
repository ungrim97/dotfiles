alias l='ls -la'
alias ls='ls --color'
alias ll='ls -l'
alias la='ls -a'
alias lal='ls -al'

# Quick way to notify me of events with a memorable name
alias say='notify-send'

# ack should actually be called ack otherwise it isn't quicker to type than
# grep!
alias ack='ack-grep'

# Git/Svn shortcuts
alias gco='git checkout'
alias gr='git rebase'
alias gro="git rebase --onto"
alias gp='git pull'
alias gl='git list'
alias gst='git status'
alias gcm='git commit -m'
alias ga='git add'

alias sst='svn status'
alias sup='svn up'

